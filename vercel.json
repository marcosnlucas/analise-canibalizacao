{
  "rewrites": [
    { 
      "source": "/analise-de-canibalizacao",
      "destination": "/index.html"
    },
    {
      "source": "/analise-de-canibalizacao/:match*",
      "destination": "/index.html"
    }
  ],
  "routes": [
    {
      "src": "/analise-de-canibalizacao/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
